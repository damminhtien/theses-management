<h2>Đồ án điểm cao</h2>
<table class="table table-hover table-striped">
    <thead>
        <tr>
            <th>STT</th>
            <th>Tên đề tài</th>
            <th>Sinh viên thực hiện</th>
            <th>Giảng viên hướng dẫn</th>
            <th>Điểm</th>
            <th>Chi tiết</th>
        </tr>
    </thead>
    <tbody>
        <% doandiemcao.forEach((data, index)=>{ %>
            <tr>
                <td><%=index+1%></td>
                <td><%=data.ten_de_tai%></td>
                <td><%=data.ten_sv%></td>
                <td><%=data.ten_gv%></td>
                <td><%=data.diem%></td>
                <td><a href="" title="Xem chi tiết"><span class="btn fa fa-eye"></span></a></td>
            </tr>
        <% }) %>
    </tbody>
</table>